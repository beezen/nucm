{
  "name": "nucm",
  "version": "1.8.7",
  "description": "Npm User Change Manager（npm 用户账号切换管理）",
  "main": "./bin/index.js",
  "scripts": {
    "release": "npm run test:coverage && npm run build && standard-version",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "dev": "rimraf dist && rollup -c rollup.config.dev.js -w",
    "build": "rimraf dist && rollup -c rollup.config.build.js"
  },
  "bin": {
    "nucm": "./bin/index.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:beezen/nucm.git"
  },
  "author": "beezen",
  "keywords": [
    "nucm",
    "npm",
    "nrm",
    "access tokens",
    "publish",
    "user change",
    "token",
    "account",
    "manager"
  ],
  "license": "MIT",
  "dependencies": {
    "colors": "^1.4.0",
    "commander": "^8.3.0",
    "conventional-changelog-cli": "^2.2.2",
    "fs-extra": "^10.0.0",
    "ini": "^2.0.0",
    "inquirer": "^8.2.0",
    "os": "^0.1.2",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/runtime": "^7.18.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/jest": "^29.5.11",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.1.1",
    "typescript": "^4.6.3"
  }
}
